@model JobEngine.Web.Areas.ScheduledJobs.Models.SelectJobTypeViewModel

@{
    ViewBag.Title = "Select Job Type";
    Layout = "~/Views/_LayoutPage.cshtml";
}



<table class="table">
    <tr>
        <th>
            Job Types
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.JobTypeItems)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Select", item.ActionLink)
            </td>
        </tr>
    }

</table>
