@model IEnumerable<JobEngine.Web.Areas.AssemblyJobs.Models.AssemblyJobViewModel>

@{
    ViewBag.Title = "Select the type of Assembly Job";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}



<table class="table">
    <tr>
        <th>
            Assembly Job Type
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.ActionLink("Select", "CreateAssemblyJob", new { id = item.AssemblyJobId })
            </td>
        </tr>
    }

</table>
